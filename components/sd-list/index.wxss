.sd-list {
  background-color: white;
  padding: 10px 16px;
  position: relative;
  width: 100%;
  box-sizing: border-box;
  line-height: 24px;
  font-size: 14px;
}
.title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 8rpx;
}
.font-color-gray {
  color: rgb(107, 119, 140);
}
.desc-label {
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 48rpx;
}
.border{
  border-top: 1rpx solid #eee;
}
