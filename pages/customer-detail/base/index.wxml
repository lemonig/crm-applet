<!--pages/deal-detail/base/index.wxml-->
<view>

  <van-cell-group>
    <van-cell title="客户名称" value="{{pageData.name}}" />
    <van-cell title="机构类型" value="{{pageData.orgTypeName}}" />
    <van-cell title="省份" value="{{pageData.base}}" />
    <van-cell title="统一社会信用代码" value="{{pageData.creditCode }}" />
    <van-cell title="地址" value="{{pageData.address}}" />
    <van-cell title="备注" value="{{pageData.description}}" />
    <van-cell title="首次录入人" value="{{pageData.acreateUserName}}" />
    <van-cell title="首次录入时间" value="{{pageData.createTime}}" />

  </van-cell-group>

  <view class="form-btn-area">
    <van-button type="info" custom-style="width: 600rpx;" bindtap="gotoedit">编辑</van-button>
  </view>
</view>